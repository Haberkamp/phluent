{
    "name": "phluent/phluent",
    "description": "This is an example project to show how to create a composer package with PHPUnit tests",
    "version": "0.1.1",
    "type": "library",
    "license": "MIT",
    "author": "Nils Haberkamp",
    "autoload": {
        "psr-4": {
            "Phluent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phluent\\Tests\\": "tests/"
        },
        "files": [
            "src/Expect.php"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "laravel/pint": "dev-main"
    }
}
