{
    "name": "phluent/phluent",
    "description": "A fluent assertion library for PHPUnit. Make your tests read the same way you speak.",
    "version": "0.5.3",
    "type": "library",
    "license": "MIT",
    "author": "Nils Haberkamp",
    "autoload": {
        "psr-4": {
            "Phluent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phluent\\Tests\\": "tests/"
        },
        "files": [
            "src/Expect.php",
            "src/Act.php"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "laravel/pint": "dev-main"
    }
}
